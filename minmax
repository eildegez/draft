#include <iostream>
using namespace std;
int main()
{
    int a[1001], n,i,maxx,minn;
    cin>>n;
    for(i=1;i<=n;i++)
        cin>>a[i];
    minn=maxx=a[1];
    for(i=1;i<=n;i++)
        {if(a[i]>maxx)
          maxx=a[i];
         if(a[i]<minn)
            minn=a[i];
        }
    cout<<minn<<" "<<maxx;
    return 0;
}
